{
    "searchPaths": [
        [ "console", "target", "learn-spark.jar" ]
    ],
    "styleSheet": [ "dependency.css" ],
    "relevantDependencyPattern": {
        "includeRegex": [ "com/seanshubin/learn/spark/core/.*" ],
        "excludeRegex": [ ]
    },
    "trimRegex": "^com/seanshubin/learn/spark/core/",
    "classFileNamePattern": {
        "includeRegex": [ ".*\\.class" ],
        "excludeRegex": [ ]
    },
    "zipFileNamePattern": {
        "includeRegex": [ ".*\\.zip", ".*\\.war", ".*\\.jar" ],
        "excludeRegex": [ ]
    },
    "generatedReportDirectory": [ "console", "target", "dependency-report" ],
    "classLimits": {
        "depth": 2,
        "complexity": 10,
        "cycleCount": 0
    },
    "packageLimits": {
        "depth": 1,
        "complexity": 0,
        "cycleCount": 0
    },
    "failIfAnyLimitExceeded": true
}
